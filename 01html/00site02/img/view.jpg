	function relateNews(){
		
		var target = $(".relate_article_n");
		var tit_height = target.find(".article_title").outerHeight();
		var news_offset = target.offset().top;
		var news_height = target.find(".relate_inner").height();
		var scr_position = news_offset + tit_height;
		var cont = $(".board_view .board_inner");
		var cont_height = cont.outerHeight();
		var cont_offset = cont.offset().top;
		var cont_position = cont_height + cont_offset;
		var last_position = cont_offset + (cont_height - target.find(".article_inner").height());
		var leng = target.find(".news_list").children("li").length;
		console.log(leng);

		$(window).scroll(function(){
			var scr = $(this).scrollTop();
			console.group("높이값");
			console.log("내용물높이",cont_height);
			console.log("오프셋",cont_offset);
			console.log("뉴스리스트높이",target.find(".article_inner").height());
			console.log(scr);
			console.groupEnd();
			if(scr >= scr_position){
				
				if(leng < 3){
					target.addClass("top");
				}else{
					target.addClass("bottom");
				}
				target.removeClass("stop");
			}else{
				target.removeClass("top");
				target.removeClass("bottom");
				target.removeClass("stop");
			}

			if(scr >= last_position){
				target.removeClass("top");
				target.removeClass("bottom");
				target.addClass("stop");
			}
		});
	};

	//10초 후 뉴스레터 구독하기 팝업 띄우기
	
